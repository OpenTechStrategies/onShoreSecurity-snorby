$.unblockUI();

<% if @sensor.present? %>
	<% if @sensor.virtual_sensor? %>
		flash_message.push({type: 'success', message: 'Black/White list applied to <%= @sensor.name %>'}); 
  <% else %>
  	flash_message.push({type: 'success', message: 'Black/White list applied to all sensors inside <%= @sensor.name %>'}); 
	<% end %>  
	flash();
<% end %>