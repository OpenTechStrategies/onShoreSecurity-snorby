<%= title "Lookup Sources" do -%>
	<%= menu_item "Add Lookup Source", new_lookup_path, 'icons/add.png' %>
<%- end -%>

<div id="lookups" class='grid_12 page boxit'>	
	<table class='default' border="0" cellspacing="0" cellpadding="0">
		<tr>		
			<th>Menu Title</th>
			<th>URL</th>
			<th class="icons">Edit</th>
			<th class="icons last">Destroy</th>
		</tr>
		<tbody class='lookups'>
			<%= render @lookups %>
		</tbody>
	</table>

	<%= pager(@lookups, '/lookups') %>	
</div>

