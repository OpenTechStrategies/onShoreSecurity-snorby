<div class="section-title plus">Rule Flowbits</div>
<div class="rule-info plus">
  <table class="rule-1" border='0' cellspacing='0' cellpadding='0'>
    <tr>
      <th class="first">Action</th>
      <th class="last">State</th>
    </tr>
    <tbody>
      <% @rule.rule_flowbits.each do |x| %>
        <tr>
          <td><strong><%= x.flowbit.name %></strong></td>
          <td><%= x.state.name if x.state.present?  %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
