<tbody class='suppress-events'>
  <% @suppress_rules.each do |f_event| %>
    <tr>
      <td><%= f_event.tracked_ip %></td>
      <td><%= f_event.tracked_type %></td>
      <% if current_user.admin %>
        <td class='icons last'><%= link_to image_tag("icons/destroy.png", :size => '16x16', :class => 'trash'), delete_filter_event_events_path(:id => f_event.id), :method => :delete, :remote => true, :confirm => "Are you sure you want to remove this IP / Network?", :disabled => true %></td>
      <% end %>
    </tr>
  <% end %>
</tbody>