<% case tab[:column].to_sym %>
<% when :CPU %>
  <%= render :partial => 'graph_metric', :locals => {:active => active, :metric => @cpu_metric, :label => tab[:column], :unit => Snorby::CONFIG_SNMP[:oids]["1.3.6.1.4.1.2021.11.11.0"]["unit"]} %>
<% when :MEMORY %>
  <%= render :partial => 'graph_metric', :locals => {:active => active, :metric => @memory_metric, :label => tab[:column], :unit => Snorby::CONFIG_SNMP[:oids]["1.3.6.1.4.1.39483.1.2.2.4.1.2.9.115.104.101.108.108.116.101.115.116.1"]["unit"]} %>
<% when :LOAD %>
  <%= render :partial => 'graph_metric', :locals => {:active => active, :metric => @load_metric, :label => tab[:column], :unit => Snorby::CONFIG_SNMP[:oids]["1.3.6.1.4.1.2021.10.1.3.2"]["unit"]} %>
<% when :HDD %>
  <%= render :partial => 'graph_metric', :locals => {:active => active, :metric => @hdd_metric, :label => tab[:column], :unit => Snorby::CONFIG_SNMP[:oids]["1.3.6.1.4.1.2021.9.1.9.1"]["unit"]} %>
<% when :SEVERITIES %>
  <%= render :partial => 'graph_severity', :locals => {:active => active} %>		
<% end %>